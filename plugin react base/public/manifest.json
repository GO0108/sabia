{
    "manifest_version": 3,
    "name": "TESTE",
    "description": "AQUI",
    "version": "1.0",
    "action": {
      "default_title": "Hello Extensions",
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["./static/js/content.js"]
      }
   ]
  }